<div id="page-cart">
    {%- if cart.item_count > 0 -%}
        <h1>{{ 'cart.filled.title' | t }}</h1>
    {%- endif -%}
    <div
        class="cart-page-contents js-cart-page-contents"
        data-cart-item-count="{{ cart.item_count }}"
        >
        {% render 'cart-contents' %}
    </div>
</div>
